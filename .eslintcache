ta(json.total);\r\n        setResulOpacity(1);\r\n        setLoading(false);\r\n    }\r\n    useEffect(()=>{\r\n        if(adList.length > 0){\r\n            setPageCount(Math.ceil(adsTotal/adList.length))\r\n            console.log(pageCount);\r\n        }else{\r\n            setPageCount(0);\r\n        }\r\n        \r\n    },[adsTotal])\r\n    useEffect(()=>{\r\n        setResulOpacity(0.3)\r\n        getAdsList();\r\n    },[currentPage])\r\n    let pagination = [];\r\n    for (let i=0;i<pageCount;i++){\r\n        pagination.push(i+1);\r\n    }\r\n    useEffect(()=>{\r\n        let queryString = [];\r\n        if(q){\r\n            queryString.push(`q=${q}`);\r\n        }\r\n        if(cat){\r\n            queryString.push(`cats=${cat}`);\r\n        }\r\n        if(state){\r\n            queryString.push(`state=${state}`);\r\n        }\r\n        history.replace({\r\n            search:`?${queryString.join('&')}`\r\n        })\r\n\r\n        if(timer){\r\n            clearTimeout(timer);\r\n        }\r\n        timer = setTimeout(getAdsList,2000);\r\n        setResulOpacity(0.3);\r\n        setCurrentPage(1);\r\n    },[q,cat,state]);\r\n\r\n    useEffect(()=>{\r\n        const getList=async()=>{\r\n            const slists= await api.getStates();\r\n            setListState(slists);\r\n        } \r\n        getList();\r\n    },[])\r\n    useEffect(()=>{\r\n        const getCategories=async()=>{\r\n            const cats= await api.getCategories();\r\n            setCategories(cats);\r\n        } \r\n        \r\n        getCategories();\r\n    },[])\r\n\r\n    return(\r\n        <PageContainer>\r\n            <PageArea>\r\n                <div className=\"leftSide\">\r\n                    <form method=\"GET\">\r\n                        <input type=\"text\" name=\"q\" placeholder=\"O que você procura?\" value={q} onChange={e=>setQ(e.target.value)}/>\r\n                        <div className=\"filterName\">Estado:</div>\r\n                        <select name=\"state\" value={state} onChange={e=>setState(e.target.value)}>\r\n                            <option></option>\r\n                            {\r\n                                listState.map((item,key)=>{\r\n                                    return <option value={item.name} key={key}>{item.name}</option>\r\n                                })\r\n                            }\r\n                        </select>\r\n                        <div className=\"filterName\">Categoria:</div>\r\n                        <ul>\r\n                            {\r\n                                categories.map((item,key)=>{\r\n                                    return <li key={key} className={cat==item.slug ? 'active':\"\"} onClick={()=>setCat(item.slug)}>\r\n                                        <img src={item.img} alt=\"\"/>\r\n                                        <span>{item.name}</span>\r\n                                    </li>\r\n                                })\r\n                            }\r\n                        </ul>\r\n                    </form>\r\n                </div>\r\n                <div className=\"rightSide\">\r\n                    <h2>Resultado</h2>\r\n                    {\r\n                        loading && adList.length === 0 &&\r\n                            <div className=\"listWarning\">\r\n                                Carregando...\r\n                            </div>\r\n                    }\r\n                    {\r\n                        !loading && adList.length === 0 &&\r\n                            <div className=\"listWarning\">\r\n                                Não encontramos resultados\r\n                            </div>\r\n                    }\r\n\r\n                    <div className=\"list\" style={{opacity:resultOpacity}}>\r\n                        {adList.map((item,key)=>{\r\n                            return <AdItem key={key} data={item}/>\r\n                        })}\r\n\r\n                    </div>